type User {
  id: ID! @id
  username: String! @unique
  email: String! @unique
  password: String!
  updatedAt: DateTime! @updatedAt
  createdAt: DateTime! @createdAt
}

type Book {
  id: ID! @id
  title: String!
  author: String!
  pic: String!
}

type Question {
  id: ID! @id
  title: String!
  body: String!
  pic: String!
}